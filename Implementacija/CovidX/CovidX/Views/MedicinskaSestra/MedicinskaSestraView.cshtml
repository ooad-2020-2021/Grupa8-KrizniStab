                                        <div class="col">
                                            <p class="labels" style="text-align:left">JMBG</p>
                                        </div>
                                        <div class="col">
                                            <p class="labels" style="text-align:right">@ViewBag.jmbg</p>
                                        </div>
                                    </div>
